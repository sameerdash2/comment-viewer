{
    "env": {
        "browser": true,
        "es2020": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "requireConfigFile": false,
        "sourceType": "module"
    },
    "rules": {
        "no-fallthrough": "off",
        "prefer-const": "warn",
        "no-unused-vars": "warn"
    },
    "globals": {
        "gtag": "readonly"
    }
}
